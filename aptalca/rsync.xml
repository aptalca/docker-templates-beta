<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Name>Rsync</Name>
  <Description>
    Rsync
  </Description>
  <Overview>
    Rsync
  </Overview>
  <Support>http://lime-technology.com/forum/index.php?topic=39413.0</Support>
  <Registry>https://hub.docker.com/r/aptalca/rsync/</Registry>
  <GitHub>https://github.com/aptalca/rsync</GitHub>
  <Repository>aptalca/rsync</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort></HostPort>
        <ContainerPort></ContainerPort>
        <Protocol></Protocol>
      </Port>
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>    
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/config</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner></Banner>
  <Icon>http://www.haykranen.nl/wp-content/uploads/2008/05/timemachine.jpg</Icon>
</Containers>
